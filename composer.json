    "phpmailer/phpmailer": "^6.9",
    "php-imap/php-imap": "^2.0"
  },
  "autoload": {
    "classmap": [
      "src/app/hrose"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
